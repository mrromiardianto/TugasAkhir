<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.screening_time">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SCREENING_TIME">
        <activity android:name=".Fitur.Menu_Dashboard"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Fitur.Menu_Login" />
        <activity android:name=".Fitur.Menu_Register"/>
            <activity android:name=".Fitur.Menu_SplashScreen" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>